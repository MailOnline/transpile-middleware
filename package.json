{
  "name": "transpile-middleware",
  "version": "0.9.5",
  "description": "Read and transpile JS code on-the-fly for browsers",
  "main": "transpile.js",
  "scripts": {
    "test": "node test",
    "kangax": "curl -s -o ./kangax/data-es6.js https://raw.githubusercontent.com/kangax/compat-table/gh-pages/data-es6.js && curl -s -o ./kangax/data-esnext.js https://raw.githubusercontent.com/kangax/compat-table/gh-pages/data-esnext.js && npm test && git commit -am 'Updated kangax files'"
  },
  "keywords": [
    "Transpile",
    "babel",
    "nodent",
    "browser",
    "ES6"
  ],
  "author": "MatAtWork, lozlow",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.5.2",
    "object.assign": "^4.0.3",
    "resolve": "^1.1.7",
    "uap-core": "git+https://github.com/ua-parser/uap-core.git",
    "uap-ref-impl": "^0.2.0",
    "yamlparser": "0.0.2"
  },
  
  "devDependencies": {
      "babel-plugin-transform-es2015-arrow-functions": "^6.5.2",
      "babel-plugin-transform-es2015-block-scoping": "^6.7.1",
      "babel-plugin-transform-es2015-computed-properties": "^6.6.5",
      "babel-plugin-transform-es2015-destructuring": "^6.6.5",
      "babel-plugin-transform-es2015-literals": "^6.5.0",
      "babel-plugin-transform-es2015-shorthand-properties": "^6.5.0",
      "babel-plugin-transform-es2015-template-literals": "^6.6.5",
      "nodent": "^2.4.0"
  },
  
  "repository": {
    "type": "git",
    "url": "https://github.com/MailOnline/transpile-middleware.git"
  },
  "engines": "node >= 0.10.0"
}
